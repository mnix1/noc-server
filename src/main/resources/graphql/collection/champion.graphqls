type Champion {
    id: ID!
    firstName: String
    nickName: String
    lastName: String
    kind: ChampionKind!
    type: CardType!
    rarity: CardRarity!
    statistics: [ChampionStatistic!]!
    cards: [ChampionCard!]!
}
